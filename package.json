{
  "name": "massive",
  "version": "2.7.3",
  "description": "A small query tool for Postgres that embraces json and makes life simpler",
  "main": "index.js",
  "bin": {
    "massive": "bin/massive.js"
  },
  "directories": {
    "example": "example",
    "test": "test"
  },
  "scripts": {
    "test": "mocha",
    "lint": "eslint .",
    "posttest": "npm run lint",
    "coverage": "istanbul cover _mocha -- -R dot"
  },
  "keywords": [
    "postgres",
    "pg",
    "postgresql"
  ],
  "author": "Rob Conery <robconery@gmail.com>",
  "contributors": [
    "Karl Seguin <karl@openmymind.net>",
    "John Atten <xivsolutions@gmail.com>",
    "Dian Fay <dian.m.fay@gmail.com>"
  ],
  "license": "BSD-3-Clause",
  "dependencies": {
    "args-js": "^0.10.12",
    "async": "^2.1.4",
    "commander": "^2.6.0",
    "deasync": "^0.1.1",
    "glob": "^7.0.5",
    "pg": "^6.4.2",
    "pg-query-stream": "^0.7.0",
    "promise-polyfill": "^6.0.2",
    "strip-bom": "^2.0.0",
    "underscore": "^1.8.2"
  },
  "devDependencies": {
    "eslint": "^2.13.1",
    "istanbul": "^0.4.2",
    "mocha": "^2.5.3",
    "sinon": "^1.12.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/robconery/massive-js.git"
  },
  "eslintConfig": {
    "extends": "eslint:recommended",
    "env": {
      "node": true,
      "mocha": true
    },
    "rules": {
      "semi": 2
    }
  }
}
