{
  "name": "massive",
  "version": "2.6.0",
  "description": "A small query tool for Postgres that embraces json and makes life simpler",
  "main": "index.js",
  "bin": {
    "massive": "bin/massive.js"
  },
  "directories": {
    "example": "example",
    "test": "test"
  },
  "scripts": {
    "test": "nyc --reporter=html --reporter=text mocha",
    "lint": "eslint '**/*.js'",
    "posttest": "npm run lint",
    "coverage": "istanbul cover _mocha -- -R dot"
  },
  "keywords": [
    "postgres",
    "pg",
    "postgresql"
  ],
  "author": "Rob Conery <robconery@gmail.com>",
  "contributors": [
    "Karl Seguin <karl@openmymind.net>",
    "John Atten <xivsolutions@gmail.com>",
    "Dian Fay <dian.m.fay@gmail.com>"
  ],
  "license": "BSD-3-Clause",
  "dependencies": {
    "co": "^4.6.0",
    "commander": "^2.6.0",
    "lodash": "^4.17.4",
    "mz": "^2.6.0",
    "pg": "^6.1.0",
    "pg-query-stream": "^1.0.0",
    "strip-bom": "^3.0.0"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "chai-as-promised": "^6.0.0",
    "co-mocha": "^1.1.3",
    "eslint": "^3.12.2",
    "mocha": "^3.2.0",
    "nyc": "^10.1.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/robconery/massive-js.git"
  },
  "jshintConfig": {
    "esversion": 6
  },
  "eslintConfig": {
    "extends": "eslint:recommended",
    "env": {
      "node": true,
      "mocha": true,
      "es6": true
    },
    "rules": {
      "semi": 2
    }
  }
}
